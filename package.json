{
  "name": "Clothly",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/compat": "^5.3.20",
    "@react-navigation/elements": "^1.3.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.1",
    "detox": "^19.7.1",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-fullscreen-box": "^1.0.0",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.2",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.5",
    "@types/jest": "^28.1.2",
    "@types/react": "^18.0.14",
    "@types/react-native": "^0.67.9",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.5.1",
    "metro-babel-register": "^0.71.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "mock-async-storage": "^2.2.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.7.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "e2e",
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation|@react-native-firebase|stick-protocol-handlers)"
    ],
    "preset": "react-native",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
